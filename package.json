  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@lib/audit(|/.*)$": "<rootDir>/libs/audit/src/$1",
      "^@lib/health(|/.*)$": "<rootDir>/libs/health/src/$1",
      "^@lib/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@lib/graphql(|/.*)$": "<rootDir>/libs/graphql/src/$1",
      "^@lib/prisma(|/.*)$": "<rootDir>/libs/prisma/src/$1",
      "^@lib/queue(|/.*)$": "<rootDir>/libs/queue/src/$1"
    }
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts",
    "schema": "prisma"
  },
  "zenstack": {
    "schema": "zenstack/schema.zmodel"
  },
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72"
