{
  "name": "nest-prisma-template-v2",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:api": "nest build api",
    "build:cli": "nest build cli",
    "build:worker": "nest build worker",
    "build:all": "concurrently 'yarn build:api' 'yarn build:cli' 'yarn build:worker'",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev:api": "nest start api --watch",
    "start:dev:cli": "nest start cli --watch",
    "start:dev:worker": "nest start worker --watch",
    "start:debug:api": "nest start api --debug --watch",
    "start:debug:cli": "nest start cli --debug --watch",
    "start:debug:worker": "nest start worker --debug --watch",
    "start:prod:api": "node dist/apps/api/main",
    "start:prod:cli": "node dist/apps/cli/main",
    "start:prod:worker": "node dist/apps/worker/main",
    "command": "node dist/apps/cli/main",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "pretest": "yarn compile",
    "posttest": "yarn lint",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:dev:create": "npx prisma migrate dev --create-only",
    "migrate:deploy": "npx prisma migrate deploy",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "npx prisma db seed"
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@nestjs/apollo": "13.0.3",
    "@nestjs/bull": "11.0.2",
    "@nestjs/common": "11.0.11",
    "@nestjs/config": "4.0.1",
    "@nestjs/core": "11.0.11",
    "@nestjs/graphql": "13.0.3",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.0.11",
    "@nestjs/swagger": "11.0.6",
    "@nestjs/throttler": "6.4.0",
    "@prisma/client": "6.4.1",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "graphql": "^16.10.0",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "nestjs-command": "^3.1.4",
    "nestjs-prisma": "^0.25.0",
    "passport": "^0.7.0",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.1",
    "prisma-class-validator-generator": "^5.0.0",
    "prisma-erd-generator": "2.0.4",
    "reflect-metadata": "0.2.2",
    "rxjs": "^7.8.2",
    "uuid": "^11.1.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.5",
    "@nestjs/schematics": "11.0.2",
    "@nestjs/testing": "11.0.11",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "8.26.0",
    "@typescript-eslint/parser": "8.26.0",
    "concurrently": "^9.1.2",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.2",
    "eslint-plugin-prettier": "5.2.3",
    "gts": "6.0.2",
    "jest": "29.7.0",
    "prettier": "3.5.3",
    "prisma": "6.4.1",
    "prisma-class-generator": "^0.2.11",
    "prisma-nestjs-graphql": "21.1.1",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.6",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@lib/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@lib/prisma(|/.*)$": "<rootDir>/libs/prisma/src/$1",
      "^@lib/graphql(|/.*)$": "<rootDir>/libs/graphql/src/$1",
      "^@lib/auth(|/.*)$": "<rootDir>/libs/auth/src/$1",
      "^@lib/queue(|/.*)$": "<rootDir>/libs/queue/src/$1"
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72"
}
