  "scripts": {
    "build:api": "nest build api",
    "build:cli": "nest build cli",
    "build:worker": "nest build worker",
    "build:all": "concurrently 'yarn build:api' 'yarn build:cli' 'yarn build:worker'",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev:api": "nest start api --watch",
    "start:dev:cli": "nest start cli --watch",
    "start:dev:worker": "nest start worker --watch",
    "start:debug:api": "nest start api --debug --watch",
    "start:debug:cli": "nest start cli --debug --watch",
    "start:debug:worker": "nest start worker --debug --watch",
    "start:prod:api": "node dist/apps/api/main",
    "start:prod:cli": "node dist/apps/cli/main",
    "start:prod:worker": "node dist/apps/worker/main",
    "command": "node dist/apps/cli/main",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "pretest": "yarn compile",
    "posttest": "yarn lint",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:dev:create": "npx prisma migrate dev --create-only",
    "migrate:deploy": "npx prisma migrate deploy",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:migrate:reset": "npx prisma migrate reset",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "npx prisma db seed --schema=schema.prisma",
    "generate": "zenstack generate && prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
